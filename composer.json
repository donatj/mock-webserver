{
	"name": "donatj/mock-webserver",
	"description": "Simple mock web server for unit testing",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Jesse G. Donat",
			"email": "donatj@gmail.com",
			"homepage": "https://donatstudios.com",
			"role": "Lead"
		}
	],
	"require": {
		"php": ">=5.4",
		"ext-sockets": "*",
		"ext-json": "*",
		"ext-curl": "*",
		"ralouphie/getallheaders": "~2.0 || ~3.0"
	},
	"autoload": {
		"psr-4": {
			"donatj\\MockWebServer\\": "src/"
		}
	},
	"require-dev": {
		"donatj/drop": "^1.0",
		"phpunit/phpunit": "~4|~9"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
