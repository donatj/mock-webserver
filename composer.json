{
	"name": "donatj/mock-webserver",
	"description": "Simple mock web server for unit testing",
	"type": "library",
	"license": "MIT",
	"keywords": [
		"webserver", "mock", "testing", "unit testing", "dev", "http", "phpunit"
	],
	"authors": [
		{
			"name": "Jesse G. Donat",
			"email": "donatj@gmail.com",
			"homepage": "https://donatstudios.com",
			"role": "Lead"
		}
	],
	"require": {
		"php": ">=7.1",
		"ext-sockets": "*",
		"ext-json": "*",
		"ralouphie/getallheaders": "~2.0 || ~3.0"
	},
	"autoload": {
		"psr-4": {
			"donatj\\MockWebServer\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Test\\": "test/"
		}
	},
	"require-dev": {
		"donatj/drop": "^1.0",
		"phpunit/phpunit": "~7|~9",
		"friendsofphp/php-cs-fixer": "^3.1",
		"squizlabs/php_codesniffer": "^3.6",
		"corpus/coding-standard": "^0.6.0 || ^0.9.0",
		"phpstan/phpstan": "^1.10",
		"ext-curl": "*"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
